---
interface Props {
  projects: { title: string; url: string }[];
}
const { projects } = Astro.props;
---

<div class="flex flex-row flex-wrap items-center gap-5">
  {
    projects.map((project) => (
      <a href={project.url} target="_blank" class="text-lg text-gray-500">
        {project.title}
      </a>
    ))
  }
</div>
